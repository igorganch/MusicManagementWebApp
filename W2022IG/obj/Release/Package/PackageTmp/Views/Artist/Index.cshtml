@model IEnumerable<W2022IG.Models.ArtistBaseViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>List of Artists</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UrlArtist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BirthName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BirthOrStartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Genre)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Executive)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.Biography)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        <img src="@Html.DisplayFor(modelItem => item.UrlArtist)" style="width: 20%; height: 20%" />
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.BirthName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.BirthOrStartDate)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Genre)
    </td>
    @*<td>
        @Html.DisplayFor(modelItem => item.Executive)
    </td>*@
    @*<td>
        @Html.DisplayFor(modelItem => item.Biography)
    </td>*@
    <td>
        @*@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |*@
        @Html.ActionLink("Details", "Details", new { id = item.Id })
        @*@Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
    </td>
</tr>
}

</table>
